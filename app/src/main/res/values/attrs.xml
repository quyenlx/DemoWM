<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundProgressBar">
        <attr name="pmc_strokeWidth" format="dimension" />
        <attr name="pmc_text" format="reference" />
        <attr name="pmc_textSize" format="dimension" />
        <attr name="pmc_drawable" format="reference" />
        <attr name="pmc_progress_color" format="reference" />
        <attr name="pmc_progress2_color" format="reference" />
        <attr name="pmc_bg_color" format="reference" />
        <attr name="pmc_progress" format="integer" />
        <attr name="pmc_progress2" format="integer" />
        <attr name="tlcp_strokeWidth" format="dimension" />
        <attr name="tlcp_text" format="reference" />
        <attr name="tlcp_textSize" format="dimension" />
        <attr name="tlcp_drawable" format="reference" />
        <attr name="tlcp_progress_color" format="reference" />
        <attr name="tlcp_progress2_color" format="reference" />
        <attr name="tlcp_bg_color" format="reference" />
        <attr name="tlcp_progress" format="integer" />
        <attr name="tlcp_progress2" format="integer" />
    </declare-styleable>

    <declare-styleable name="DayView">
        <attr name="firstDayOfWeek" format="enum">
            <enum name="sunday" value="1" />
            <enum name="monday" value="2" />
            <enum name="tuesday" value="3" />
            <enum name="wednesday" value="4" />
            <enum name="thursday" value="5" />
            <enum name="friday" value="6" />
            <enum name="saturday" value="7" />
        </attr>
        <attr name="hourHeight" format="dimension" />
        <attr name="eventTextSize" format="dimension" />
        <attr name="headerColumnTextColorSunday" format="color" />
        <attr name="headerColumnTextColorSaturday" format="color" />
        <attr name="headerColumnTextColorWeek" format="color" />
        <attr name="lineColor" format="color" />
        <attr name="highlightLineColor" format="color" />
        <attr name="noOfVisibleDays" format="integer" />
        <attr name="showFirstDayOfWeekFirst" format="boolean" />
        <attr name="todayBackgroundColor" format="color" />
        <attr name="eventTextColor" format="color" />
        <attr name="xScrollingSpeed" format="float" />
        <attr name="eventCornerRadius" format="dimension" />
        <attr name="horizontalFlingEnabled" format="boolean" />
        <attr name="verticalFlingEnabled" format="boolean" />
        <attr name="scrollDuration" format="integer" />
        <attr name="eventPadding" format="dimension" />
    </declare-styleable>
</resources>